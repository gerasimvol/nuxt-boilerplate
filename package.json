{
  "name": "nuxt-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js starter kit",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node .nuxt/dist/main.js",
    "generate": "nuxt generate",
    "analyze": "nuxt build --analyze",
    "pm2": "pm2 start ecosystem.config.js",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint \"./**/*.scss\" \"./**/*.vue\"",
    "lint": "npm run lint:js && npm run lint:css",
    "deploy": "pm2 stop 0 && git pull origin dev && npm i && npm run build && pm2 start 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@epegzz/sass-vars-loader": "^6.0.0",
    "@koa/cors": "^3.0.0",
    "@nuxtjs/axios": "^5.9.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "ctx-polyfill": "^1.1.4",
    "dotenv": "^8.2.0",
    "gsap": "^3.2.6",
    "koa": "^2.11.0",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "nuxt": "^2.12.1",
    "object-fit-images": "^3.2.4",
    "object-fit-videos": "^1.0.4",
    "to-pascal-case": "^1.0.0",
    "vue-click-outside": "^1.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "backpack-core": "^0.8.4",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.3",
    "ip": "^1.1.5",
    "loader-runner": "^3.1.0",
    "node-sass": "^9.0.0",
    "node-sass-utils": "^1.1.2",
    "nodemon": "^2.0.2",
    "request": "^2.88.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "terser-webpack-plugin": "^2.3.5",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.16.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "FirefoxAndroid >= 64",
    "ChromeAndroid >= 70",
    "iOS >= 11",
    "Chrome >= 60",
    "Opera >= 44",
    "Firefox >= 60",
    "ie 11",
    "edge >= 17",
    "Safari >= 10"
  ]
}
